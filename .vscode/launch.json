  "version": "0.2.0",
  "configurations": [
    {
      "name": "api-server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/apiserver/main.go",
      "args": ["-c", "${workspaceFolder}/config.yaml"],
      "env": {
        "SERVICE_NAME": "api-server",
        "API_SERVER_SERVER_BIND": ":8090"
        // "API_SERVER_CONFIG_PATH": "${workspaceFolder}/config.yaml"
      }
    },
    {
      "name": "init",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/apiserver/main.go",
      "args": ["init"],
      "env": {
        "SERVICE_NAME": "qqlx",
        "QQLX_CONFIG_PATH": "${workspaceFolder}/config.yaml"
      }
    }
  ]
}
